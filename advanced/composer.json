{
  "name": "yiisoft/yii2-app-advanced",
  "version": "1.0.0",
  "description": "Yii 2 Advanced Project Template",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "project template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "GPL-2.0",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=8.4",
    "yiisoft/yii2": "~2.0.51",
    "yiisoft/yii2-bootstrap": "~2.0.11",
    "dmstr/yii2-adminlte-asset": "2.6.2",
    "mdmsoft/yii2-admin": "~2.0",
    "rmrevin/yii2-fontawesome": "~2.17.1",
    "2amigos/qrcode-library": "~1.0",
    "yiisoft/yii2-bootstrap4": "^2.0",
    "w7corp/easywechat": "6.0.0",
    "markrogoyski/math-php": "2.*",
    "bizley/jwt": "^4.0",
    "lcobucci/jwt": "^5.0",
    "lcobucci/clock": "^3.2",
    "yiisoft/yii2-redis": "^2.0",
    "zircote/swagger-php": "^4.0",
    "doctrine/annotations": "^2.0",
    "tencentcloud/sts": "^3.0",
    "yiisoft/yii2-symfonymailer": "^4.0",
    "predis/predis": "^3.3"
  },
  "replace": {
    "npm-asset/bootstrap": ">=4.2.1",
    "npm-asset/vue": "~2.5.3"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "^2.2",
    "yiisoft/yii2-faker": "~2.0.0",
    "phpunit/phpunit": ">=6.5.5",
    "codeception/verify": "~1.0.0",
    "symfony/browser-kit": "^4.2 || ^5.0",
    "codeception/codeception": "^5.3",
    "codeception/module-yii2": "^2.0",
    "codeception/module-asserts": "^3.3",
    "codeception/module-filesystem": "^3.0"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "easywechat-composer/easywechat-composer": true
    }
  }
}
